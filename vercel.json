{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "functions": {
        "api/index.php": {
            "runtime": "@vercel/php@0.7.0"
        },
        "public/index.php": {
            "runtime": "@vercel/php@0.7.0"
        }
    },
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:AeA3/RbAhDXoHnQ8Ctu5faRf83N5rDv/fTgatF5jvdU=",
        "SESSION_DRIVER": "cookie",
        "CACHE_STORE": "array"
    }
}